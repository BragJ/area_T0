record(ao, "$(P)$(R)AcquireTime")
{
   field(DTYP,"Async Soft Channel")
   #field(PINI, "YES")
   field(DTYP, "Soft Channel")
   field(PREC, "3")
   field(VAL,  "1.0")
   field(DRVH, "65.0")
   field(DRVL, "0.00005")
   field(OUT, "$(P)$(R)AcquireTimeCalc.A CA")
}

# Transform the AcquireTime parameter in seconds
# to the ExposureTimeRaw in microseconds
record(calcout, "$(P)$(R)AcquireTimeCalc")
{
   field(DTYP,"Async Soft Channel")
   field(CALC, "A*1000000")
   #field(INPA, "$(P)$(R)AcquireTime CP")
   field(OUT, "$(P)$(R)ExposureTimeRaw CA")
   field(OOPT, "Every Time")
   field(DOPT, "Use CALC")
}

record(longout, "$(P)$(R)ExposureTimeRaw") {
  field(OMSL,  "closed_loop")
  field(DOL, "$(P)$(R)AcquireTimeCalc.VAL CP")
  field(DRVH, "65000000")
  field(DRVL, "50")
}

record(ai, "$(P)$(R)AcquireTime_RBV")
{
   field(DTYP, "Raw Soft Channel")
   field(INP,  "$(P)$(R)ExposureTimeRaw_RBV CP")
   field(PREC, "3")
   field(SCAN, "Passive")
   field(PINI, "YES")
   field(ASLO, "1e-06")
   field(AOFF, "0")
}


