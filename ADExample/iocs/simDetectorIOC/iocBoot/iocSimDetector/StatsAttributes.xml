<?xml version="1.0" standalone="no" ?>
<!-- Attributes -->
<Attributes
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://epics.aps.anl.gov/areaDetector/attributes ../attributes.xsd"
    >
    <Attribute name="ComputeStatistics" type="PARAM" source="COMPUTE_STATISTICS"  datatype="INT"    description="Compute statistics"/>
    <Attribute name="BgdWidth"          type="PARAM" source="BGD_WIDTH"           datatype="INT"    description="Background width"/>
    <Attribute name="MinValue"          type="PARAM" source="MIN_VALUE"           datatype="DOUBLE" description="Minimum value"/>
    <Attribute name="MinX"              type="PARAM" source="MIN_X"               datatype="DOUBLE" description="Minimum X"/>
    <Attribute name="MinY"              type="PARAM" source="MIN_Y"               datatype="DOUBLE" description="Minimum Y"/>
    <Attribute name="MaxValue"          type="PARAM" source="MAX_VALUE"           datatype="DOUBLE" description="Maximum value"/>
    <Attribute name="MaxX"              type="PARAM" source="MAX_X"               datatype="DOUBLE" description="Maximum X"/>
    <Attribute name="MaxY"              type="PARAM" source="MAX_Y"               datatype="DOUBLE" description="Maximum Y"/>
    <Attribute name="MeanValue"         type="PARAM" source="MEAN_VALUE"          datatype="DOUBLE" description="Mean value"/>
    <Attribute name="SigmaValue"        type="PARAM" source="SIGMA_VALUE"         datatype="DOUBLE" description="Sigma value"/>
    <Attribute name="Total"             type="PARAM" source="TOTAL"               datatype="DOUBLE" description="Total"/>
    <Attribute name="Net"               type="PARAM" source="NET"                 datatype="DOUBLE" description="Net"/>
    <Attribute name="ComputeCentroid"   type="PARAM" source="COMPUTE_CENTROID"    datatype="INT"    description="Compute Centroid"/>
    <Attribute name="CentroidThreshold" type="PARAM" source="CENTROID_THRESHOLD"  datatype="DOUBLE" description="Centroid threshold"/>
    <Attribute name="CentroidX"         type="PARAM" source="CENTROIDX_VALUE"     datatype="DOUBLE" description="Centroid X position"/>
    <Attribute name="CentroidY"         type="PARAM" source="CENTROIDY_VALUE"     datatype="DOUBLE" description="Centroid Y position"/>
    <Attribute name="SigmaX"            type="PARAM" source="SIGMAX_VALUE"        datatype="DOUBLE" description="Sigma X"/>
    <Attribute name="SigmaY"            type="PARAM" source="SIGMAY_VALUE"        datatype="DOUBLE" description="Sigma Y"/>
    <Attribute name="SigmaXY"           type="PARAM" source="SIGMAXY_VALUE"       datatype="DOUBLE" description="Sigma XY"/>
</Attributes>
    
    
